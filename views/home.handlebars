<h3>Lista de Postagens</h3>


{{#each posts}}
	<small>{{this.dataValues.createdAt}}</small>
	<h1>{{this.dataValues.titulo}}</h1>
	<p>{{this.dataValues.conteudo}}</p>
	<a href="deletar/{{this.dataValues.id}}"><button>excluir</button></a>
	<a href="editar/{{this.dataValues.id}}"><button>editar</button></a>
	<hr>
{{/each}}